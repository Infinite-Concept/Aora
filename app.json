{
  "expo": {
    "name": "Aora",
    "slug": "Aora",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-small.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo-small.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "Aora",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.iife.aora"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-small.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.iife.aora"
    },
    "web": {
      "favicon": "./assets/logo-small.png"
    },
    "plugins": ["expo-router", "expo-font"],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "59f17325-4c55-4ed0-aff9-6a7cfbdedcb0"
      }
    }
  }
}
